{
  "name": "electron-vue-vite",
  "version": "2.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --mode production&& electron-builder",
    "build:win": "vue-tsc --noEmit && vite build --mode production&& electron-builder --win",
    "build:mac": "vue-tsc --noEmit && vite build --mode production&& electron-builder --mac",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.2",
    "element-plus": "^2.4.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "sortablejs": "^1.15.1",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "mitt": "^3.0.1",
    "screenfull": "^6.0.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/qs": "^6.9.10",
    "@types/sortablejs": "^1.15.7",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.3.3",
    "autoprefixer": "^10.4.16",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.7",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.4",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
